<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PropertySurvey.OrTemplate"
             Title="Drawing">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="Center" Spacing="20">
            <Button Text="New Drawing" WidthRequest="230" Clicked="OnNew" />
            <Button Text="Template" WidthRequest="230" Clicked="OnTemp" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>